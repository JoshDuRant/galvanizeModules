function getLengthOfShortestElement(arr) {
  // your code here
  if (arr.length === 0) {
      return 0;
  }
  let shortestString = arr[0];
  for (let i = 1; i < arr.length; i++) {
      if (arr[i].length < shortestString.length) {
        shortestString = arr[i];
      }
  }
  return shortestString.length;
}
