function computeCompoundInterest(principal, interestRate, compoundingFrequency, timeInYears) {
  // your code here
  let numberOfTimesCompounded = compoundingFrequency * timeInYears;
  let finalAmount = principal * (1 + (interestRate/compoundingFrequency)) ** numberOfTimesCompounded;
  let compoundedInterest = finalAmount - principal;
  return compoundedInterest;
} 
