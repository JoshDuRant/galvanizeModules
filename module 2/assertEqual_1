// FUNCTION DEFINITION(S)
function square(n) {
  return n * n;
}

// ASSERTION FUNCTION(S) TO BE USED

function assertEqual(expected, actual, testName) {
    if (actual === expected) {
        console.log(`passed`);
    } else {
        console.log (`FAIL ${testName} answer should be "${expected}"`)
    }
    
}

